public class Samurai extends Human{
    public Samurai () {
        super("Samurai", 3, 3, 3, 200)
    }
    public Integer deathBlow(Human target){
        target.health = 0;
        this.health -= this.health/2;
        return this.health;
        //How do I return the dead enemy, too?
    }
    public Integer meditate(){
        this.health += this.health/2;
        return this.health;
    }
    public Integer howMany(){
        this.health -= 10;
        return this.health;
        //I think I have to write something for += sum whenever a Samurai is created?
    }
}

